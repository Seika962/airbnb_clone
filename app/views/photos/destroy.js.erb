$("#photos").html("<%= j render 'photo_list' %>");
var val = "<%= @room.photos.blank? %>";
var ready = "<%= is_ready?(@room) %>";

if (val == "true") {
    $("#photo-check").hide();
}
if (ready == "true") {
    $("#btn-publish").attr('disabled',false);
} else {
    $("#btn-publish").attr('disabled',true);
}
